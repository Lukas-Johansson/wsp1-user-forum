{% extends "layout.njk" %}
{% block content %}
<main class="login">
    <h1>{{ title }}</h1>
    <form action="/login" method="POST">
    <input type="text" name="username">
    <input type="password" name="password">
    <button type="submit">Login</button>
    </form>
    Don't have a user? register <a href="/register">here</a>
</main>

{% endblock %}